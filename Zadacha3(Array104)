#include <iostream>

using namespace std;

int main() {
    int n, k, m;
    cin >> n;
    int* a;
    a = new int[n];
    cin >> m >> k;
    cout << endl << "a:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    n += m;
    for (int i = n - 1; i >= k + m - 1; i--) {
        a[i] = a[i - m];
    }
    for (int i = k - 1; i < k + m - 1; ++i) {
        a[i] = 0;
    }
    for (int i = 0; i < n; i++) {
        cout << a[i] << " ";
    }
}
